{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "pulsar/carousel",
  "version": "0.1.0",
  "title": "Carousel",
  "category": "design",
  "textdomain": "pulsar",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "viewScript": "file:./view.js",
  "render": "file:./render.php",
  "attributes": {
    "carouselSettings": {
      "type": "object",
      "default": {
        "type": "slide",
        "drag": true,
        "perPage": 3,
        "perMove": 1,
        "gap": "30px",
        "arrows": true,
        "pagination": true,
        "autoplay": false,
        "interval": 5000,
        "breakpoints": {
          "640": {
            "perPage": 1,
            "perMove": 1,
            "arrows": true,
            "pagination": false,
            "gap": "30px"
          },
          "1024": {
            "perPage": 2,
            "perMove": 1,
            "arrows": true,
            "pagination": false,
            "gap": "30px"
          }
        }
      }
    },
    "advancedCarouselSettings": {
      "type": "object"
    },
    "mergeSettings": {
      "type": "boolean",
      "default": false
    },
    "ariaLabel": {
      "type": "string",
      "default": "Carousel"
    },
    "hasList": {
      "type": "boolean",
      "default": true
    },
    "allowedBlocks": {
      "type": "array",
      "default": [
        "pulsar/carousel-slide"
      ]
    },
    "template": {
      "type": "array",
      "default": []
    },
    "templateLock": {
      "type": "string",
      "default": false
    }
  }
}