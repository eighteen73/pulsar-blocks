{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "pulsar/carousel",
  "version": "0.1.0",
  "title": "Carousel",
  "category": "design",
  "textdomain": "pulsar",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "script": "file:./script.js",
  "render": "file:./render.php",
  "attributes": {
    "splide": {
      "type": "object",
      "default": {
        "type": "slide",
        "perPage": 2,
        "gap": "2rem",
        "arrows": true,
        "pagination": true
      }
    }
  }
}