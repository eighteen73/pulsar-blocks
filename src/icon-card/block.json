{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "pulsar/icon-card",
	"version": "0.1.0",
	"title": "Icon Card",
	"category": "media",
	"icon": "image-filter",
  "attributes": {
    "imageId": {
      "type": "number"
    },
    "url": {
      "type": "string"
    },
    "openInNewTab": {
      "type": "boolean",
      "default": false
    }
  },
  "styles": [
    {
      "name": "default",
      "label": "Default",
      "isDefault": true
    }
  ],
	"supports": {
		"html": false
	},
	"textdomain": "pulsar",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"render": "file:./render.php"
}
