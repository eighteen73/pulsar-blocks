{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "pulsar/accordion-item",
	"version": "0.1.0",
	"title": "Accordion Item",
	"category": "design",
	"parent": ["pulsar/accordion"],
	"supports": {
		"inserter": false,
		"reusable": false,
		"html": false
	},
	"attributes": {
    "title": {
      "type": "string",
			"selector": ".wp-block-pulsar-accordion__heading"
		},
		"id": {
      "type": "string",
      "default": ""
		}
	},
	"usesContext": ["level"],
  "textdomain": "pulsar-blocks",
	"editorScript": "file:./index.js",
	"render": "file:./render.php"
}
