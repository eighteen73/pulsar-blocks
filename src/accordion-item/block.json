{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "pulsar/accordion-item",
	"version": "0.1.0",
	"title": "Accordion Item",
	"category": "design",
	"parent": [ "pulsar/accordion" ],
  "supports": {
    "inserter": false,
    "reusable": false,
    "html": false
  },
	"textdomain": "pulsar",
  "attributes": {
    "title": {
      "type": "string",
      "selector": ".wp-block-pulsar-accordion__heading"
    },
    "id": {
      "type": "string"
    }
  },
	"editorStyle": "file:./index.css",
	"editorScript": "file:./index.js",
  "style": "file:./style-index.css",
  "render": "file:./render.php"
}
