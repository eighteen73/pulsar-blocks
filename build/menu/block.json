{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "version": "1.0.0",
  "name": "pulsar/menu",
  "title": "Menu",
  "category": "theme",
  "description": "Display a menu from a menu location.",
  "icon": "menu",
  "attributes": {
    "location": {
      "type": "string",
      "default": ""
    },
    "collapse": {
      "type": "string",
      "default": "never"
    },
    "orientation": {
      "type": "string",
      "default": "horizontal"
    },
    "hasSubmenuBack": {
      "type": "boolean",
      "default": false
    },
    "hasSubmenuLabel": {
      "type": "boolean",
      "default": false
    },
    "hasViewAll": {
      "type": "boolean",
      "default": false
    },
    "submenuOpensOnClick": {
      "type": "boolean",
      "default": false
    }
  },
  "supports": {
    "html": false,
    "interactivity": true
  },
  "variations": [
    {
      "name": "responsive-menu",
      "title": "Responsive Menu",
      "attributes": {
        "collapse": "small-only",
        "hasSubmenuBack": true,
        "opensOnClick": false
      }
    }
  ],
  "textdomain": "pulsar-blocks",
  "editorScript": "file:./index.js",
  "style": "file:./style-index.css",
  "render": "file:./render.php",
  "viewScriptModule": "file:./view.js"
}