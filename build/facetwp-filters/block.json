{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "pulsar/facetwp-filters",
  "description": "Display FacetWP filters for post type listings.",
  "version": "1.0.0",
  "title": "FacetWP Filters",
  "category": "widgets",
  "icon": "filter",
  "allowedBlocks": [
    "pulsar/facetwp-filter"
  ],
  "supports": {
    "html": false,
    "reusable": false,
    "inserter": true,
    "lock": true,
    "multiple": false,
    "align": true,
    "spacing": {
      "margin": true
    }
  },
  "attributes": {
    "id": {
      "type": "string"
    },
    "filterLayout": {
      "type": "string",
      "default": "slide-out"
    },
    "collapsable": {
      "type": "boolean",
      "default": true
    }
  },
  "providesContext": {
    "id": "id",
    "filterLayout": "filterLayout",
    "collapsable": "collapsable"
  },
  "textdomain": "pulsar",
  "editorScript": "file:./index.js",
  "viewScriptModule": "file:./view.js",
  "render": "file:./render.php",
  "style": "file:./style-index.css"
}