{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "pulsar-blocks/carousel",
  "version": "0.1.0",
  "title": "Carousel",
  "category": "text",
  "icon": "universal-access-alt",
  "attributes": {
    "slidesPerPage": {
      "type": "number",
      "default": 1
    },
    "slidesPerMove": {
      "type": "number",
      "default": 1
    },
    "isAutoplayEnabled": {
      "type": "boolean",
      "default": false
    },
    "isShowArrowsEnabled": {
      "type": "boolean",
      "default": false
    },
    "isPaginationEnabled": {
      "type": "boolean",
      "default": false
    },
    "animationMode": {
      "type": "string",
      "default": "loop"
    },
    "slideGap": {
      "type": "number",
      "default": 0
    },
    "focusPosition": {
      "type": "number",
      "default": 1
    },
    "focusType": {
      "type": "string",
      "default": "center"
    }
  },
  "example": {
    "attributes": {
      "message": "Example Dynamic Block"
    }
  },
  "supports": {
    "html": false
  },
  "textdomain": "pulsar-blocks",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css"
}