{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "pulsar/filter-facetwp",
  "description": "Display a filter for post type listings using FacetWP.",
  "version": "1.0.0",
  "title": "FacetWP Filter",
  "category": "woocommerce",
  "icon": "filter",
  "parent": [
    "pulsar/filters"
  ],
  "supports": {
    "html": false,
    "reusable": false,
    "inserter": true,
    "lock": true,
    "multiple": true
  },
  "attributes": {
    "facetName": {
      "type": "string"
    }
  },
  "usesContext": [
    "layout",
    "id"
  ],
  "textdomain": "pulsar",
  "editorScript": "file:./index.js",
  "viewScriptModule": "file:./view.js",
  "render": "file:./render.php",
  "style": "file:./style-index.css"
}