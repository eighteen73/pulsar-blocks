{
	"name": "eighteen73/pulsar-blocks",
	"description": "A collection of blocks we use at eighteen73.",
	"type": "wordpress-plugin",
	"keywords": [
		"wordpress",
		"plugin",
		"blocks",
		"gutenberg",
		"pulsar"
	],
	"license": "MIT",
	"authors": [
		{
			"name": "Brett Mason",
			"email": "brett@eighteen73.co.uk"
		}
	],
	"minimum-stability": "stable",
	"require": {
		"php": ">=7.4"
	},
	"require-dev": {
		"eighteen73/wordpress-coding-standards": "^2.0.0"
	},
	"config": {
		"allow-plugins": {
			"dealerdirect/phpcodesniffer-composer-installer": true
		}
	},
	"scripts": {
		"lint": "./vendor/bin/phpcs",
		"format": "./vendor/bin/phpcbf"
	}
}
